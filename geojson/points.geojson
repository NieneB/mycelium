---
---
{
  "type": "FeatureCollection",
  "features": [
    {% for point in site.data.points %}
    {
      "type": "Feature",
      "geometry": {
        "type": "Point",
        "coordinates": {{ point.coordinates | reverse | jsonify }}
      },
      "properties": {
        "title": "{{ point.title }}",
        "theme": "{{ point.theme }}",
        "url": "{{ point.url }}"
      }
    }
    {% if forloop.last == false %}
    ,
    {% endif %}
    {% endfor %}
  ]
}
